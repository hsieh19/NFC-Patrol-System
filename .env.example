DATABASE_URL="file:./dev.db"
# =================================================================
# 飞书开放平台配置 (Feishu/Lark Configuration)
# =================================================================

LARK_APP_ID=cli_xxxxxxxxxxxx
LARK_APP_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxx
NEXT_PUBLIC_LARK_APP_ID=cli_xxxxxxxxxxxx

# =================================================================
# 数据库切换 (Database Selection)
# =================================================================

# 切换开关: sqlite 或 mysql
# 使用 sqlite 时，数据库文件会自动生成在 prisma/dev.db
DB_TYPE=sqlite

# =================================================================
# MySQL 详细参数 (仅在 DB_TYPE=mysql 时生效)
# =================================================================

MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=password
MYSQL_DATABASE=xungeng

# =================================================================
# 系统运行环境配置
# =================================================================

NODE_ENV=development
